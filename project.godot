[application]

config/name="BugOff"
run/main_scene="res://TowerDefense/_Scenes/MainMenu.tscn"
config/icon="res://TowerDefense/Art/bug_logo.png"
config/windows_native_icon="res://TowerDefense/Art/bug_icon_big.ico"
boot_splash/show_image=false

[autoload]

Global="*res://TowerDefense/Scripts/global.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/size/always_on_top=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[input]

camera_pan={
